{
    "configurations": [
        {
            "name": "shell",
            "includePath": [
                "${workspaceFolder}/include/**",
                "${workspaceFolder}/source/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "windowsSdkVersion": "10.0.18362.0",
            "compilerPath": "/usr/bin/g++",
            "cStandard": "c11",
            "cppStandard": "c++20",
            "intelliSenseMode": "gcc-x64",
            "compilerArgs": [
                "-fconcepts",
                "-std=c++2a",
                "-Wall",
                "-Wextra",
                "-c",
                "-g",
                "-O0"
            ]
        }
    ],
    "version": 4
}