{
    "files.associations": {
        "ostream": "cpp"
    },
    "c-cpp-compile-run.cpp-flags": "-Wall -Wextra -std=c++2a -fconcepts -g",
    "c-cpp-compile-run.run-args": "-g -lf log/Logger.log test/t1.is",
    "terminal.integrated.allowMnemonics": true,
    "terminal.integrated.confirmOnExit": true,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.enableBell": true,
    "terminal.integrated.experimentalUseTitleEvent": true,
    "C_Cpp.default.compilerPath": "/usr/bin/g++",
    "C_Cpp.experimentalFeatures": "Enabled",
    "remote.WSL.debug": true,
    "remote.WSL.fileWatcher.polling": true,
    "code-runner.clearPreviousOutput": true,
    "code-runner.customCommand": "make all",
    "code-runner.fileDirectoryAsCwd": true,
    "code-runner.runInTerminal": true,
    "code-runner.saveAllFilesBeforeRun": true,
    "code-runner.saveFileBeforeRun": true
}